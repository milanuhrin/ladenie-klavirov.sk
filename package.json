{
  "name": "my-gatsby-site",
  "version": "1.0.5",
  "private": true,
  "description": "My Gatsby Site",
  "author": "rasto",
  "keywords": [
    "gatsby"
  ],
  "scripts": {
    "dev": "gatsby develop -H 0.0.0.0",
    "start": "gatsby develop -H 0.0.0.0",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "deploy": "gatsby-plugin-s3 deploy --yes",
    "bd": "gatsby build && gatsby-plugin-s3 deploy --yes",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "framer-motion": "^5.3.3",
    "gatsby": "^4.4.0",
    "gatsby-plugin-fontawesome-css": "^1.2.0",
    "gatsby-plugin-google-analytics": "^4.4.0",
    "gatsby-plugin-image": "^2.4.0",
    "gatsby-plugin-pnpm": "^1.2.10",
    "gatsby-plugin-postcss": "^5.4.0",
    "gatsby-plugin-react-helmet": "^5.4.0",
    "gatsby-plugin-s3": "^0.3.8",
    "gatsby-plugin-sharp": "^4.4.0",
    "gatsby-plugin-sitemap": "^5.4.0",
    "gatsby-source-filesystem": "^4.4.0",
    "gatsby-transformer-sharp": "^4.4.0",
    "prettier-plugin-tailwindcss": "^0.1.7",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "usehooks-ts": "^2.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/eslint-parser": "^7.16.5",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/line-clamp": "^0.3.1",
    "@types/node": "^16.11.9",
    "@types/react": "^17.0.36",
    "@types/react-helmet": "^6.1.5",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.1",
    "@typescript-eslint/parser": "^5.10.1",
    "autoprefixer": "^10.4.2",
    "babel-eslint": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-tailwindcss": "^3.3.2",
    "gatsby-plugin-root-import": "^2.0.8",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.3",
    "postcss": "^8.4.5",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.15",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "4.5.2",
    "webpack": "^5.0.0"
  },
  "lint-staged": {
    "**/*.{js,ts,tsx}": [
      "eslint ./src/**/*.{js,ts,tsx}",
      "git add"
    ],
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  }
}